{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "lib": [ "dom", "es2015", "es2017" ],
    "target": "es6",
    "moduleResolution": "Node"
  }
}